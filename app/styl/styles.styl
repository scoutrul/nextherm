@import url('https://fonts.googleapis.com/css?family=Exo+2:500,600,300&subset=cyrillic,latin')

@import url('https://fonts.googleapis.com/css?family=PT+Serif:400,400italic&subset=latin,cyrillic')



$whitebg = #F4FDFB
$blue = #0063b1
$green = #1D9870
$greybg = #ECECEC
$greenbg = #EAFFE5
$blacktxt = #232323


$cta(color)
	border-radius 35px
	background-color color
	color #fff
	font-size 60%
	text-transform uppercase
	padding 5px 15px
	box-shadow 1px 1px 1px #000
	text-align center
	display table
	margin 80px auto
	line-height 50px
	letter-spacing 0.08em
	max-width 280px
	width 100%
	cursor pointer
	transition .1s ease-in
	&:hover
		background-color lightness(color, 60%)

*
	position relative
	z-index 2
	color inherit
	letter-spacing 0.03em
	font-weight inherit

html
	overflow-x hidden
	height 100%
	
body
	font-size 18px
	padding 0
	margin 0
	height 100%
	font-family 'Exo 2', sans-serif
	font-weight 300
	max-width 100vw
	font-size 1.4rem
	background-color #FFF


h1, h2, h3, h4, h5, ul, li
	margin 0
	padding 0
	
h1, h2, h3
	font-family 'PT Serif', serif
	margin 50px auto
	color $blue
	letter-spacing 0.06em
	border-bottom 7px solid $blue
	display: table
	
h1
	font-size 280%

	
.container
	min-width 960px
	max-width 1240px
	margin auto
	z-index 3
	padding 40px 30px 90px 


.nt
	margin-right 20px
	padding: 0 10px
	background-color: rgba(255, 255, 255, 0.09)
	font-family 'PT Serif', serif
	font-style italic
	&:after
		content 'TM'
		display inline-block
		vertical-align super
		font-size 30%
		position absolute
		top -15px
		right -15px
		

#certificate, #protocole
	cursor pointer




// Pop-up window
#show-box
	cursor pointer
#show-box, #cta-box
	display none
	z-index 10
	position fixed
	height 100vh
	width 100vw
	min-height 600px
	min-width 100%
	top 0
	left 0
	background-size contain
	background-position 50%
	background-repeat no-repeat
	background-color rgba(0, 0, 0, 0.64)
	
	
	.close
		cursor pointer
		z-index: 11;
		color: #fff;
		font-size: 320%;
		position: absolute;
		top: 1%;
		right: 3%;
		width: 50px;
		height: 80px;
		&:after
			z-index 11
			content: '+'
			color: #FFF
			position: absolute
			transform: rotate(45deg)
			text-shadow: 11px 1px 7px #000
			
	form
		width: 600px
		height: 80%
		// min-height 600px
		margin: auto
		top: 5%
		background-color: #FFF
		padding: 30px
		border-radius: 2px
		display: flex
		flex-direction: column
		justify-content: space-between
		font-size: 80%
		box-shadow 0 11px 30px rgba(0, 0, 0, 0.57)
		
		input, textarea
			padding 5px
			margin 5px 0
		h2
			text-align: center
			width: 100%
			padding-bottom: 50px
			
		.send_mail
			min-height: 60px
			background-color: $blue
			border: 0
			color: #FFF
			margin 20px 0
			&:hover
				background-color lightness($blue, 60%)
				
section#production.isMobile
	&:before,&:after
		display none
		content initial
		width 0
		
section.screen1
	&:before
		background none
		
