.quote-rotate 
	position: absolute;
	top: 10px;
	opacity: 0;
	overflow: visible;
	visibility: hidden;
	transition: opacity, 0.3s, ease;
	padding-bottom: 10px;

	&:before 
		content: '"';
		position: absolute;
		top: -15px;
		left: -25px;
		font-size: 200%;
		font-style: italic;
	&:after 
		content: '"';
		position: absolute;
		bottom: -5px;
		left: -25px;
		font-size: 200%;
		font-style: italic;
		
	img
		float left
		padding: 10px 20px

