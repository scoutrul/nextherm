// animations



// form error
@keyframes formError 
	0% 
		background-color: rgba(239, 5, 5, .3)
	100% 
		background-color: rgba(255, 255, 255, 1)
	
.form_error 
	transition .3s ease-in
	animation formError .3s
//




// 
@keyframes rotate 
	0% 
		opacity 0
		transform rotateY(10deg) scale(.3)
		animation-timing-function ease-in
	100% 
		opacity 1
		transform rotateY(360deg) scale(1)
		animation-timing-function ease-out
		
	
@keyframes fadein 
	0% 
		opacity 0
		top 20px
	100% 
		opacity 1
		top 0


.rotate
	animation-delay .4s
	animation-name: rotate 
	animation-duration: 1s 
	animation-iteration-count: 1
	animation-fill-mode: forwards;
	
.fadein
	animation-delay .4s
	animation-name: fadein 
	animation-duration: 1s 
	animation-iteration-count: 1
	animation-fill-mode: forwards;
	

